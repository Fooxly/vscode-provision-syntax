{
	"name": "vscode-provision-syntax",
	"displayName": "Provision Syntax",
	"description": "Providing a simple way to view all notes in your workspace based on keywords.",
	"author": "Fooxly",
	"publisher": "Fooxly",
	"license": "MIT",
	"icon": "assets/icon.png",
	"keywords": [
		"fooxly",
		"vscode",
		"code",
		"todo",
		"fixme",
		"note",
		"notes",
		"keyword",
		"highlighting",
		"syntax"
	],
	"homepage": "https://github.com/Fooxly/vscode-provision-syntax",
	"repository": {
		"type": "git",
		"url": "https://github.com/Fooxly/vscode-provision-syntax.git"
	},
	"bugs": {
		"url": "https://github.com/Fooxly/vscode-provision-syntax/issues",
		"email": "bugs@fooxly.com"
	},
	"galleryBanner": {
		"color": "#272729",
		"theme": "dark"
	},
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [ 
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
			"command": "provision.help",
			"title": "Help: Provision"
		}],
		"configuration": {
			"type": "object",
			"title": "Provision",
			"properties": {
				"provision.keywords": {
					"type": "object",
					"title": "Keywords",
					"description": "Keywords to look for with a specific configuration"
				},
				"provision.groups": {
					"type": "array",
					"title": "Keyword groups",
					"description": "Keyword groups to use",
					"items": {
						"type": "object",
						"title": "Keyword group"
					}
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/underscore": "^1.9.3",
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"underscore": "^1.9.1"
	}
}
